@model EternityWebServiceApp.ViewModels.DataReferenceViewModel
@{
    ViewData["Title"] = "Удаление связи";
}
<h2>@ViewData["Title"]</h2>

<div class="table-responsive">
    <table class="table">
        <tr>
            <th>ID</th>
            <th>Город</th>
            <th>Достопримечательность</th>
        </tr>
        <tr>
            <td>@Model.DataReferenceId</td>
            <td>@Model.CityId</td>
            <td>@Model.AttractionId</td>
        </tr>
    </table>
</div>
 
<form asp-action="Delete" asp-controller="DataReference" method="post">
    <div class="form-group">
        <input type="submit" class="btn btn-outline-secondary" value="Удалить" />
    </div>
</form>

<div>
    <a asp-controller="City" asp-action="Index">Вернуться к списку</a>
</div>