@model EternityWebServiceApp.Models.User
@{
    ViewData["Title"] = "Удаление пользователя";
}
<h2>ViewData["Title"]</h2>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Email</th>
        <th>Никнейм</th>
        <th>Роль</th>
    </tr>
    <tr>
        <td>@Model.UserId</td>
        <td>@Model.Email</td>
        <td>@Model.UserName</td>
        <td>@ViewData["RoleName"]</td>
    </tr>
</table>
 
<form asp-action="Delete" asp-controller="User" method="post">
    <div class="form-group">
        <input type="submit" class="btn btn-outline-secondary" value="Удалить" />
    </div>
</form>

<div>
    <a asp-controller="User" asp-action="Index">Вернуться к списку</a>
</div>