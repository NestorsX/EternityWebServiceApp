@model EternityWebServiceApp.Models.Attraction
 
@{
    ViewData["Title"] = "Новая достопримечательность";
}
<h2>@ViewData["Title"]</h2>
 
<form asp-action="Create" asp-controller="Attraction" method="post" enctype="multipart/form-data">
    <div class="validation" asp-validation-summary="ModelOnly"></div>
    <div>
        <div class="form-group">
            <label asp-for="Title">Достопримечательность</label>
            <input type="text" asp-for="Title" class="form-control form-control-sm" />
            <span asp-validation-for="Title"></span>
        </div>
        <div class="form-group">
            <label asp-for="Description">Описание</label>
            <input type="text" asp-for="Description" class="form-control form-control-sm" />
            <span asp-validation-for="Description"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Сохранить" class="btn btn-outline-secondary" />
        </div>
    </div>
</form>
 
<div>
    <a asp-controller="Attraction" asp-action="Index">Вернуться к списку</a>
</div>