@model EternityWebServiceApp.Models.GameScore
@{
    ViewData["Title"] = "Удаление лучшего результата";
}
<h2>@ViewData["Title"]</h2>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Пользователь</th>
        <th>Игра</th>
        <th>Счет</th>
    </tr>
    <tr>
        <td>@Model.GameScoreId</td>
        <td>@Model.User.UserName</td>
        <td>@Model.Game.Name</td>
        <td>@Model.Score</td>
    </tr>
</table>
 
<form asp-action="Delete" asp-controller="GameScore" method="post">
    <div class="form-group">
        <input type="submit" class="btn btn-outline-secondary" value="Удалить" />
    </div>
</form>

<div>
    <a asp-controller="GameScore" asp-action="Index">Вернуться к списку</a>
</div>